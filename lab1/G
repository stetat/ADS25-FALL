#include <iostream>
#include <stack>
#include <vector>
#include <deque>
#include <string>
#include <cmath>

using namespace std;

bool isprime(int x) {
    if ( x == 1 ) return false;

    for(int i = 2; i * i <= x; i++) {
        if ( x % i == 0) {
            return false;
        }
    }
    return true;
}



int main() {
    int n, k = 0, c = 1, p = 2;
    cin >> n;

    while(true) {

        if ( n == k) {
            cout << p;
            break;
        }

        p++;

        if (isprime(p)) {
            c++;
            if (isprime(c)) {
                k++;
            }
        }

     


    }

    
    
    




   
    


    return 0;

}