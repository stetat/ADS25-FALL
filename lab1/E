#include <iostream>
#include <stack>
#include <vector>
#include <deque>
#include <string>
#include <cmath>

using namespace std;




int main() {

    deque<int> boris;
    deque<int> nursik;
    int c, moves = 0;

    for(int i = 0; i<10; i++) {
        cin >> c;
        if ( i < 5) {
            boris.push_back(c);
        }

        else {
            nursik.push_back(c);
        }

    }

    while (true) {
        if (moves > pow(10, 6)) {
            cout<<"blin nichya"<<endl;
            break;
        }

        if (boris.empty()) {
            cout << "Nursik " << moves;
            break;
        }

        if (nursik.empty()) {
            cout<<"Boris " << moves;
            break;
        }

        int b = boris.front(); 
        int n = nursik.front();
        boris.pop_front();
        nursik.pop_front();

        

        if ( ( b == 0 && n == 9 ) || (b > n && !(b == 9 && n == 0))) {
            boris.push_back(b);
            boris.push_back(n);
        
            moves++;
        }

        else if ( ( b == 9 && n == 0 ) || (b < n && !(b == 0 && n == 9))) {
            nursik.push_back(b);
            nursik.push_back(n);
            
            moves++;
        }

       

    }

    
    




   
    


    return 0;

}