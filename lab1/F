#include <iostream>
#include <stack>

using namespace std;

bool isprime(int x) {
    for(int i = 2; i*i <= x ; i++) {
        if (x % i == 0) {
            return false;
        }
        
    }
    
    return true;
    
}

int main() {
    int c = 1, s = 2;
    int n;
    cin >> n;
    
    while ( true ) {
        
        if ( c == n ) {
            break;
        }
        
        
        else {
            s++;
            if ( isprime(s) ) {
            c++;
            }
            
        }
        
        
        
        
        
        
        
    }
    
    
    
    cout<<s<<endl;
    
    
    
}