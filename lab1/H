#include <iostream>
#include <stack>

using namespace std;

bool isprime(int x) {
    for(int i = 2; i*i <= x ; i++) {
        if (x % i == 0) {
            return false;
        }
        
    }
    
    return true;
    
}

int main() {
    int n;
    cin >> n;
    
    
    
    if (!isprime(n) || n == 1) {
        cout << "NO";
    }
    
    else {
        cout<<"YES";
    }
    
    
    
}
